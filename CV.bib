
@techreport{nudesocInformeResultadosOficial2020,
  title = {Informe de Resultados Oficial {{Encuesta Zona Cero}}},
  author = {{Nudesoc}},
  year = {2020},
  month = apr,
  institution = {{Open Science Framework}},
  doi = {10.31219/osf.io/76mdz},
  abstract = {Con el principal objetivo de producir evidencia emp\'irica sobre la revuelta popular de octubre de 2019 en Chile, se desarroll\'o una encuesta representativa de l-s manifestantes de la Plaza de la Dignidad, con la finalidad de levantar informaci\'on que caracterice sus demandas y percepciones. Se incluyen preguntas por las demandas recogidas desde los cabildos por Unidad Social, violencia policial, satisfacci\'on con la democracia y la trayectoria de l-s manifestantes en torno a la participaci\'on previa en protestas.},
  type = {Preprint}
}

